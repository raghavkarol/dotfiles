    parser = argparse.ArgumentParser(formatter_class=argparse.RawDescriptionHelpFormatter,
                                     description='')
    parser.add_argument('--example',
                        help='')
    subparsers = parser.add_subparsers(title='commands',
                                       metavar='cmd [opts]',
                                       help='commands, use --help to see help')
