# -*- mode: snippet -*-
# name: ct_suite
# key: ct_suite
# --
-module($1_SUITE).

-compile([export_all, nowarn_export_all]).

-include_lib("common_test/include/ct.hrl").

%% ---------------------------------------------------------
%% CT callbacks
%% ---------------------------------------------------------
suite() ->
    [{timetrap, {seconds, 5}}].

init_per_suite(Config) ->
    Config.

end_per_suite(_Config) ->
    ok.

init_per_testcase(_TestCase, Config) ->
    Config.

end_per_testcase(_TestCase, _Config) ->
    ok.

all() ->
    [$2].

%% ---------------------------------------------------------
%% Tests
%% ---------------------------------------------------------
$2(_Config) ->
    ok.
